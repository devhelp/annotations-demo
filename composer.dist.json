{
    "name": "dh-php-annotations",
    "repositories": [
        {
            "type":"package",
            "package": {
                "name": "mindplay-dk/php-annotations",
                "version": "1.1.0", //odpowiada temu co jest w require
                "source": {
                    "url": "git://github.com/mindplay-dk/php-annotations.git",
                    "type": "git",
                    //odpowiada tagowi/galezi jaka ma pobrać z repo
                    //jeżeli zmieni się to composer nie wykryje zmainy i nie zupdateuje
                    //(tylko to co jest w requires/version jest dla niego wazne(
                    "reference":"1.1.0"
                },
                "target-dir": "Mindplay",
                "autoload": {
                    "psr-0": { "Mindplay\\": "" }
                }
            }
        }
    ],
    "require" : {
        "symfony/class-loader": "~2",
        "doctrine/common": "*",
        "jms/di-extra-bundle": "*",
        "mindplay-dk/php-annotations" : "1.1.0"
    }
}